{
	"name": "vanham.io",
	"manifest": {
		"server": {
			"app": {
				"slogan": "Website"
			}
		},
		"connections": [
			{
				"port": 8080,
				"labels": ["web"]
			}
		],
		"plugins": [
			{"scooter": {}},
			{"inert": {}},
			{"blankie":{
				"styleSrc": ["self", "unsafe-inline"]
			}},

			{
				"./api/static": [
					{
						"select": ["web"],
						"options": {}
					}
				]
			},
			{
				"./api/web": [
					{
						"select": ["web"],
						"options": {}
					}
				]
			},
			{
				"./api/blog": [
					{
						"select": ["web"],
						"options": {}
					}
				]
			}
		]
	},
	"database": {
		"dsn": "mongodb://root@localhost/vanham.io"
	},
	"api": {
		"version": "v1"
	}
}
